{
  "name": "qryo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "pnpm --filter \"./packages/**\" run clean",
    "preinstall": "node -e \"if(process.env.CI == 'true') {console.log('Skipping preinstall...'); process.exit(1)}\" || npx -y only-allow pnpm",
    "install:csb": "pnpm install --frozen-lockfile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config rollup.config.js && pnpm --filter \"./packages/**\" run build && pnpm run build:types",
    "build:types": "pnpm --filter \"./packages/**\" run build:types",
    "dev": "pnpm run watch",
    "watch": "concurrently --kill-others \"rollup --config rollup.config.js -w\" \"pnpm run build --watch\"",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --plugin-search-dir . \"{packages,examples}/**/src/**/*.{md,js,jsx,ts,tsx,json,vue,svelte}\"",
    "prettier:write": "pnpm run prettier --write",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "keywords": [],
  "author": "ctholho",
  "license": "MIT",
  "namespace": "@akronym",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^18.15.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "7.x",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^3.19.1",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unocss": "^0.50.6",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vitepress": "1.0.0-alpha.60"
  },
  "dependencies": {
    "pm2": "^5.3.0"
  }
}
